MODULE main
VAR
 a : boolean;
 b : boolean;
ASSIGN
 init(a) := TRUE;
 init(b) := FALSE;
 next(a) := case
  a & b : TRUE;
  a & !b & next(b) : {TRUE, FALSE};
  a & !b & !next(b) : FALSE;
  !a : FALSE;
 esac;
 next(b) := case
  a & b : FALSE;
  a & !b : {TRUE, FALSE};
  !a: TRUE;
 esac;

LTLSPEC G(a)
LTLSPEC G(a U b)
LTLSPEC a U X (a & !b)
LTLSPEC X !b & G(!a | !b)
LTLSPEC X(a & b) & F(!a & !b)
SPEC EF EG a
SPEC EF EG b
LTLSPEC F ((G a) | (G b))
